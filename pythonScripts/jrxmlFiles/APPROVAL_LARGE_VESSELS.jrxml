			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="60" width="426" height="8" uuid="80d57ef1-2df0-412d-8f87-a18ef894f574"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="false"/>
				</textElement>
				<textFieldExpression><![CDATA["To file and secure with this Authority an Application for Amendment of its Certificate of Public Convenience (CPC) to include the subject ship in the company’s fleet, prior to its deployment in the domestic trade; <i>(This condition is only applicable for ships engage in domestic shipping operation)</i>"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="68" width="426" height="8" uuid="d0b9dc8b-4cd4-450e-9135-082d43424763"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="false"/>
				</textElement>
				<textFieldExpression><![CDATA["To maintain accreditation under MARINA Circular No. 2006-003 and to continuously comply with its provisions, such as, capitalization requirement during the operation of the subject ship in the domestic trade; <i>(This condition is only applicable for ships engage in domestic shipping operation)</i>"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="76" width="426" height="8" uuid="db759ab3-f11b-4ec6-8dde-1d00debf290e">
					<printWhenExpression><![CDATA[!$P{otherInfo.utilization}.equals( "Domestic Shipping" )]]></printWhenExpression>
				</reportElement>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="false"/>
				</textElement>
				<textFieldExpression><![CDATA["That the "+$F{vesselType}+" shall be used exclusively for "+$P{otherInfo.utilization}+" only and shall not be used for domestic shipping operation; <i>(This condition is applicable for ships engaging on company/personal use only)</i>"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="84" width="426" height="8" uuid="ec812c6b-6dec-42cc-8c70-d06d87ce088d"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA["To maintain at all times a ship’s class throughout her operation in the domestic trade;"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="108" width="426" height="8" uuid="e1d5a7cc-9d69-434c-862c-e92965b429dd"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="false"/>
				</textElement>
				<textFieldExpression><![CDATA["That the company shall request issuance of “Clearance” from the Domestic Shipping Sector (DSS), this Authority, relative to the compliance with the submission of post-approval documents, prior to the subject ship’s permanent registration under Philippine flag to be filed in the MARINA Regional Office (MRO);"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="100" width="426" height="8" uuid="575025a5-2b0f-43ee-9700-c620dec15050"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="false"/>
				</textElement>
				<textFieldExpression><![CDATA["To comply with the rules governing the mandatory marine insurance to cover liabilities arising from pollution and wreck removal under MARINA Circular No. 2009-22 once suspension of its implementation is lifted;"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="116" width="426" height="8" uuid="c65bd867-3245-4751-9605-ece2fd020659"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA["To be held liable for payment of fines and penalties under pertinent MARINA Circulars specifically the revocation of the Letter-Approval or disqualification to register the imported ships upon findings of fake/spurious submission of documents;"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="92" width="426" height="8" uuid="687eebdb-56cb-43c5-80f5-42b648177908"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="false"/>
				</textElement>
				<textFieldExpression><![CDATA["To continuously comply with the International Safety Management (ISM) Code, as implemented by this Authority under MARINA Memorandum Circular No. 2015-11;"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="140" width="426" height="8" uuid="f0c0632e-65fa-491a-87f9-622601dbfa3a"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="false"/>
				</textElement>
				<textFieldExpression><![CDATA["To comply with all applicable laws, circulars, rules and regulations, maritime safety regulations as regards to hull, machinery, navigational, lifesaving and fire-fighting equipment."]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="132" width="426" height="8" uuid="53e1f85a-3e21-428b-b789-df47422e33ca"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="false"/>
				</textElement>
				<textFieldExpression><![CDATA["Comply with all laws, rules and regulations governing the operation of Philippine-registered ships; and,"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="124" width="426" height="8" uuid="8a0f40bd-b719-49ec-aac4-c354287d59ec"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="false"/>
				</textElement>
				<textFieldExpression><![CDATA["To comply with all relevant rules and regulations of the Bureau of Customs and other concerned government agencies; and,"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="196" width="426" height="8" uuid="2d6fd2bc-b440-4c57-ab71-826e2a233127"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA["To be classed by a government-recognized or an accredited classification society prior to issuance of safety-related certificates or prior to operation in the domestic trade."]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="172" width="426" height="8" uuid="c6eb3cd8-1a5d-4961-a2e8-d85acd8d436d"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12"/>
				</textElement>
				<textFieldExpression><![CDATA["To subject the ship to inspection by a MARINA surveyor for the following purposes, prior to her registration under Philippine flag:"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="164" width="426" height="8" uuid="c6ddfe1c-165b-40a0-bb45-65990356c0a2"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12"/>
				</textElement>
				<textFieldExpression><![CDATA["To submit safety related documents to be required by the Port State Control;"]]></textFieldExpression>
			</textField>
			<textField textAdjust="StretchHeight">
				<reportElement positionType="Float" x="55" y="156" width="426" height="8" uuid="2f857dca-298c-4020-a237-48ef4cbc1b34"/>
				<box topPadding="12"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="Arial-CE" size="12" isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA["Pursuant to M.C. 2015-07, the company shall submit the ship plans to the Shipyard Regulation Sector (SRS) for review and approval in case there will be a conversion, alteration, modification of the ship;"]]></textFieldExpression>
